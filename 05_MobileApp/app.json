{
  "expo": {
    "name": "Avyantra_mobile-app",
    "slug": "Avyantra_mobile-app",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "4.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.avyantra.com",
      "versionCode": 5
    },
    "extra": {
      "URL": "<BACKEND_URL>",
      "SCORE_URL": "<ONLINE_SCOREAPI_URL>"
    }
  }
}
